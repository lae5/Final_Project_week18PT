package com.promineotech.candle.entity;

import java.util.List;
import org.hibernate.validator.constraints.Length;
import jakarta.validation.constraints.Null;
import lombok.Data;





@Data
public class OrderRequest {
	
	

	
	
	  @Null
	  @Length(max = 30)
	  @jakarta.validation.constraints.Pattern(regexp = "[\\w\\s]*")
	  private String customer;
	  
	  @NotNull
	  private CandleType candle_type;
	  
	  @NotNull
	  private ScentName scent;
	  
	  @NotNull
	  private SizeType size_type;
	  
	 

	  private List<@NotNull @Length(max = 30) @Pattern(regexp = "[\\w\\s]*") 
	  String> options;
	


}
